<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.highradius.model.Lang" table="language">
	    <id name="language_id" type="integer">
	      <column name="language_id"/>
	      <generator class="identity"/>
	    </id>

		<property name="name">
			<column name="name"/>
		</property>
		<set name="movie" table="film" cascade="all" inverse="true" lazy="false" fetch="select"  >
            <key>
                <column name="language_id" not-null="true" />
            </key>
            <one-to-many class="org.highradius.model.Lang" />
        </set>
            
	</class>
</hibernate-mapping>