<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.highradius.model.Movie" table="film">
	    <id name="film_id" type="integer">
	      <column name="film_id"/>
	      <generator class="identity"/>
	    </id>
	    <property name="rating" type="string" >
	    	<column name="rating" />
	    </property>
	    <property name="special_features" type="string">
	    	<column name="special_features"/>
	    </property>
	     <property name="director" type="string">
	    	<column name="director"/>
	    </property>
	    <property name="release_year" type="string">
	    	<column name="release_year"/>
	    </property>
	    <property name="description" type="string">
	    	<column name="description"/>
	    </property>
	    <property name="title" type="string">
	    	<column name="title" not-null="true"/>
	    </property>
	    
	    <many-to-one class="org.highradius.model.Lang" cascade="all" lazy="false" fetch="select" name="language">
			<column name="language_id" not-null="true"  />
	    </many-to-one>
	    <property name="isDeleted" type="boolean" >
	    	<column name="isDeleted" not-null="true"/>
	    </property>
	  
	</class>
</hibernate-mapping>