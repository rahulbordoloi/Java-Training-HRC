<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
   "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

	<package name="default" extends="json-default">
		<action name="DemoStruts" class="com.highradius.struts.action.DemoAction">	
			<result name="success" type="json">
				<param name="root">result</param>
			</result>	
			<result name="error">./error.jsp</result>
		</action>
	</package>

	<package name="SakilaUI" extends="json-default">

		<action name="GetSakila" class="com.highradius.struts.action.SakilaGetAction" method="getData">	
			<result name="success" type="json">
				<param name="root">dataResponse</param>
			</result>
			<result name="error">./error.jsp</result>	
		</action>

		<action name="AddSakila" class="com.highradius.struts.action.SakilaAddAction" method="addData">	
			<result name="success">/index.jsp</result>	
			<result name="error">./error.jsp</result>	
		</action>

		<action name="EditSakila" class="com.highradius.struts.action.SakilaEditAction" method="editData">	
			<result name="success">/index.jsp</result>	
			<result name="error">./error.jsp</result>	
		</action>

		<action name="DeleteSakila" class="com.highradius.struts.action.SakilaDeleteAction" method="deleteData">	
			<result name="success">/index.jsp</result>	
			<result name="error">./error.jsp</result>		
		</action>

	</package>

	<package name="SakilaUITemp" extends="json-default">

		<action name="GetSakilax" class="com.highradius.struts.action.SakilaAction" method="getData">	
			<result name="success" type="json">
				<param name="root">dataResponse</param>
			</result>
			<result name="error">./error.jsp</result>	
		</action>

		<action name="AddSakilax" class="com.highradius.struts.action.SakilaAction" method="addData">	
			<result name="success">/index.jsp</result>	
			<result name="error">./error.jsp</result>	
		</action>

		<action name="EditSakilax" class="com.highradius.struts.action.SakilaAction" method="editData">	
			<result name="success">/index.jsp</result>	
			<result name="error">./error.jsp</result>	
		</action>

		<action name="DeleteSakilax" class="com.highradius.struts.action.SakilaAction" method="deleteData">	
			<result name="success">/index.jsp</result>	
			<result name="error">./error.jsp</result>		
		</action>

	</package>

</struts>
